<%= form_for @rent do |f| %>
    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :price %>
    <%= f.number_field :price %>

    <%= f.label :room %>
    <%= f.number_field :room %>

    <%= f.label :bath %>
    <%= f.number_field :bath %>

    <%= f.label :property %>
    <%= f.text_field :property %>

    <%= f.label :available %>
    <%= f.date_field :available %>

    <%= f.label :image %>
    <%= f.url_field :image %>

    <%= f.label :agent_id %>

    <%= f.label :agent_id %>
    <%= f.select :agent_id, Agent.pluck(:title, :id) %>

    <%= f.submit %>
<% end%>